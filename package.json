{
  "name": "app-skeleton",
  "version": "0.0.1",
  "description": "Skeleton app for a DHIS2 web app",
  "main": "src/index.html",
  "scripts": {
    "validate": "npm ls --depth 0",
    "start": "webpack-dev-server",
    "lint": "./node_modules/eslint/bin/eslint.js src test example",
    "test": "./node_modules/karma-cli/bin/karma start test/karma.config.js --single-run",
    "test-watch": "./node_modules/karma-cli/bin/karma start test/karma.config.js",
    "test-cov": "npm test",
    "posttest-cov": "open ./coverage/phantomjs/lcov-report/index.html",
    "build": "make app"
  },
  "keywords": [
    "DHIS2",
    "App"
  ],
  "author": "Mark Polak",
  "license": "BSD-3-Clause",
  "dependencies": {},
  "devDependencies": {
    "babel": "5.8.34",
    "babel-core": "5.8.34",
    "babel-eslint": "4.1.1",
    "babel-loader": "5.4.0",
    "chai": "3.4.1",
    "css-loader": "0.18.0",
    "eslint": "1.3.1",
    "eslint-config-dhis2": "0.0.5",
    "eslint-plugin-react": "3.3.1",
    "isparta-loader": "1.0.0",
    "karma": "0.13.15",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.1.0",
    "karma-sourcemap-loader": "0.3.6",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "node-sass": "3.3.2",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "sass-loader": "2.0.1",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.3",
    "susy": "2.2.5",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.10.1"
  },
  "pre-commit": [
    "test",
    "lint",
    "validate"
  ]
}
